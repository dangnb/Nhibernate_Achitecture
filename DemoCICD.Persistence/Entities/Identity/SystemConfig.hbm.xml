<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping   xmlns="urn:nhibernate-mapping-2.2">
  <class name="QLTS.Domain.Entities.Identity.SystemConfig, QLTS.Domain" table="SystemConfig" lazy="true" >
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
    <property name="ComID">
      <column name="ComID" sql-type="int" not-null="false" />
    </property>
    <property name="Code">
      <column name="Code" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Value">
      <column name="Value" sql-type="nvarchar" not-null="false" />
    </property>

  </class>
</hibernate-mapping>